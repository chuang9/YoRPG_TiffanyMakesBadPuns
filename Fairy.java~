public class Fairy extends Character{
  public Fairy() {
	health = ((int)((Math.random()*100)+50));
        strength = ((int)((Math.random()*100)+55));
        defense = ((int)((Math.random()*30)+15));;
        atkRating = (Math.random());
    }

  public Fairy(String n) {
        this();
        name = n;
    }
}
